{
    "name": "freenas-10-gui",
    "version": "0.3.0",
    "dependencies": {
        "collections": "montagejs/collections#master",
        "montage": "montagejs/montage#d90c72866f433d849a55594741ce7fcf8da3dbb0",
        "blue-shark": "freenas/blue-shark#14fc0450d56162534d2114a8e462a5465783756d",
        "montage-data": "montagestudio/montage-data#e3208b8b55da686950dd67c4f2f727dc0800c8b2",
        "digit": "montagejs/digit",
        "validators": "~0.3.1"
    },
    "devDependencies": {
        "montage-testing": "~0.4.0",
        "commander": "~2.9.0",
        "websocket": "~1.0.22",
        "prompt": "~0.3.0",
        "mr": "montagejs/mr#53633e56f3e9d00c5a311d993098ddd89ed9be80",
        "rimraf": "2.5.2"
    },
    "exclude": [
        "run-tests.html",
        "test"
    ],
    "bundle": [ "ui/main.reel" ],
    "scripts": {
        "generate-descriptors" : "node ./tools/generate.js descriptors -t ./core/model/descriptors",
        "generate-models" : "node ./tools/generate.js models ./core/model/descriptors/ -p ./core/model/descriptors -t ./core/model",
        "generate-enumerations" : "node ./tools/generate.js enumerations -t ./core/model/enumerations",
        "warnings": "node ./tools/generate.js descriptors -w --no-save"
    }
}
