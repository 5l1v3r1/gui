{
  "blueprint_containersection_virtualmachines": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "virtualMachines",
      "blueprint": {"@": "root"}
    }
  },
  "blueprint_containersection_templates": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "templates",
      "blueprint": {"@": "root"}
    }
  },
  "blueprint_containersection_docker": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "docker",
      "blueprint": {"@": "root"}
    }
  },
  "root": {
    "prototype": "core/model/model-descriptor",
    "properties": {
      "name": "ContainerSection",
      "propertyBlueprints": [
        {"@": "blueprint_containersection_virtualmachines"},
        {"@": "blueprint_containersection_templates"},
        {"@": "blueprint_containersection_docker"}
      ]
    }
  }
}
